INSTALL:

	# checkout the source code
	git clone git@github.com:pancubs/renpan.git renpan
	cd renpan

	# create the database
	createdb renpan
	python
	>> import schema
	>> schema.init()

	# run the unit tests
	make test

	# start the webserver
	make run
